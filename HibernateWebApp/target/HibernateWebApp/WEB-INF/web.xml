<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>EMSWebApp</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>EmployeeFindAllController</display-name>
		<servlet-name>EmployeeFindAllController</servlet-name>
		<servlet-class>com.createiq.ems.controller.EmployeeFindAllController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeFindAllController</servlet-name>
		<url-pattern>/findAll</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>EmployeeSaveController</display-name>
		<servlet-name>EmployeeSaveController</servlet-name>
		<servlet-class>com.createiq.ems.controller.EmployeeSaveController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeSaveController</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>

</web-app>